              "community_id": community_id,
			  "start":"0",
			  "num":"5",
			  "recommend":"1"
            };

  $.ajax({
		type:"POST",
		url:'/app_dev.php/api/top/retrieveByFilter',
		dataType:"json",
		data:$.toJSON(json),
		timeout:5000,
		cache:true,
		async:false,
		success: function (data, textStatus) {
			for(var i=0;i<data.length;i++){
				$("#community_a").append("<tr><td><a onclick='to_other("+data[i].landz_id+");' style='color:black;'>"+data[i].title+"</a></td><td style='width:20%;'>"+data[i].sell_price+"万</td></tr>");
				}
