ul{
	list-style-type:none;	
}
#nav ul li{
	margin-right:20px;
	float:left;
	display:inline-block;
}
#magic-line{
	position:absolute;bottom:-7px;left:0;width:100px;height:3px;background:#999
}
.ie6 #nav li, .ie7 #nav li{
	display:inline
}
.ie6 #magic-line {
	bottom:-3px
}
#nav ul li a:link{
	color:#fff;
}
#nav ul li a:hover{
	color:#CCC;
	text-decoration:none;
}
#nav ul li a:visited{
	color:#FFF;
	background-image:url(../../images/nav_bg.png);
}
#nav ul li a:active{
	color:#FFF;
}
#nav_right{
	float:right;
	line-height:42px;
}
#search_box{
	width: 487px;
	padding: 7px 0 5px 15px;
	border: 1px solid #cdcdcd;
	border-right: 0;
	color: #999;
	border-radius: 3px;
	outline: none;
	vertical-align: top;
}
#search_close {
	height: 17px;
	position: absolute;
	top: 10px;
	width: 17px;
	right:280px;
	display: inline-block;
	background:url(../../images/x.png) 0 0 no-repeat;
}
.tabs {
	height: 35px;
	width: 300px;
	border: 1px solid #cdcdcd;
	border-bottom: none;
	border-radius: 3px 3px 0 0;
	position: relative;
	
}
.tabs ul li {
	float: left;
	line-height: 35px!important;
	height: 35px!important;
	font-size: 15px;
	font-weight: bolder;
	font-family:微软雅黑;
	text-align:center;
	background-position: 20px center;
}
.tabs ul li.first {
	width:120px;
	height:36px;
	position: absolute;
	top: 1px;
	left: 0;
	background: #FFF;
	border-bottom:1px solid #fff;
	z-index:6000;
}
.tabs ul li.first #category_list_div2 {
	line-height: 40px;
	height: 17px;
}
.tabs ul li.last {
	width:185px;
	border-left: 1px solid #cdcdcd;
	font-weight: normal;
	margin-left: 80px;
	_display: inline;
}
.tabs ul li.last a{
	background-image:url(../../images/location.png);
	background-repeat: no-repeat;
	padding-left: 18px;
	_padding-left: -3px;
	text-decoration:none;
	color: #666!important;
	vertical-align: -1px;
}
#category {
	border: 1px solid #dbdbdb;
	z-index:12;
	font-family:微软雅黑;
}
#category ul li{
	line-height:30px;
}
.red{
	color:#F00;
}
.bold{
	font-weight:bold;
}
#list_img_ul img{
	width:68px;
	height:53px;
}
#right_ul2 li{
	line-height:40px;
}
#right_ul2 li span{
	color:#F00;
	padding-right:15px;
}